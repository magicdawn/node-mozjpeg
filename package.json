{
  "name": "node-mozjpeg",
  "version": "0.1.0",
  "description": "mozjpeg node binding",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "src",
    "lib",
    "install",
    "types/index.d.ts",
    "binding.gyp",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "mocha && tsd",
    "test-cover": "nyc --reporter=lcov --reporter=text mocha",
    "report-cover": "codecov",
    "gen-readme": "swig render ./readme/readme.md > README.md && prettier --write README.md",
    "install": "prebuild-install --runtime=napi || (node install/build-dep.js && node-gyp rebuild)",
    "prebuild": "prebuild --runtime=napi"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/node-mozjpeg.git"
  },
  "keywords": [
    "mozjpeg",
    "binding",
    "compress"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/node-mozjpeg/issues"
  },
  "homepage": "https://github.com/magicdawn/node-mozjpeg#readme",
  "devDependencies": {
    "@magicdawn/eslint-config": "latest",
    "@magicdawn/prettier-config": "latest",
    "@types/node": "^14.0.13",
    "codecov": "^3.3.0",
    "eslint": "^6.8.0",
    "husky": "latest",
    "lint-staged": "latest",
    "mocha": "^6.1.4",
    "napa": "^3.0.0",
    "nyc": "^14.0.0",
    "prettier": "latest",
    "sharp": "^0.25.4",
    "should": "^13.2.3",
    "tsd": "^0.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,less,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "napa": {
    "mozjpeg-src": "mozilla/mozjpeg#v3.3.1"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^3.0.0",
    "prebuild-install": "^5.3.4"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  }
}
